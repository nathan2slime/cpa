{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "pipeline": {
    "build": {
      "cache": false,
      "dependsOn": ["^build"],
      "env": ["NEXT_PUBLIC_*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "start": {
      "cache": false
    },
    "seed": {
      "cache": false
    },
    "prod": {
      "cache": false
    },
    "dev": {
      "inputs": [".env"],
      "cache": false
    },
    "generate": {
      "cache": false
    },
    "migrate:dev": {
      "cache": false
    },
    "migrate:deploy": {
      "cache": false
    }
  }
}
