{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "dotenv -- turbo run build",
    "test": "dotenv -- turbo run test",
    "clean": "rm -rf node_modules",
    "start": "dotenv -- turbo run start",
    "dev": "dotenv -- turbo run dev",
    "seed": "dotenv -- turbo run seed",
    "prod": "dotenv -- turbo run prod",
    "generate": "dotenv -- turbo run generate",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "migrate:dev": "dotenv -- turbo run migrate:dev",
    "migrate:deploy": "dotenv -- turbo run migrate:deploy",
    "migrate:reset": "dotenv -- turbo run migrate:reset",
    "seed:run": "dotenv -- turbo run seed",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "^20.11.28",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^2.30.0",
    "dotenv-cli": "^7.1.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "latest",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "turbo": "^1.13.3",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@4.1.1",
  "resolutions": {
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "installConfig": {
    "hoistingLimits": "none"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nathan2slime/dengue_app.git"
  },
  "license": "MIT",
  "dependencies": {
    "firebase": "^10.12.2"
  }
}
